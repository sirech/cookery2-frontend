{
  "name": "cookery2",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "react-scripts build",
    "linter:js": "npx eslint --config .eslintrc.js 'src/**/*.{ts,tsx}' --max-warnings 0",
    "linter:css": "npx stylelint src/**/*.css",
    "linter": "npm run linter:js && npm run linter:css",
    "linter:fix": "npm run linter:js -- --fix --format codeFrame && npm run linter:css --fix",
    "format": "prettier --write \"**/*.+(ts|tsx|json|css|scss)\"",
    "start": "PORT=3003 react-scripts start",
    "pretest": "tsc --noEmit",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "watch": "react-scripts test --verbose=false",
    "bundle": "npm run build && source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "auth0-js": "^9.14.0",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "formik": "^2.2.0",
    "formik-material-ui": "^3.0.0",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^15.3.4",
    "typescript": "4.0.3",
    "yup": "^0.29.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/auth0-js": "^9.14.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/ramda": "^0.27.29",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/recompose": "^0.30.7",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.0",
    "coveralls": "^3.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.4.2",
    "pact": "^4.3.2",
    "prettier": "^2.1.2",
    "react-scripts": "3.4.3",
    "source-map-explorer": "^2.5.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0"
  },
  "proxy": "http://localhost:4003",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --config .eslintrc.js --max-warnings 0",
    "*.scss": "./go linter-css"
  }
}
