{
  "name": "cookery2",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "build": "react-scripts build",
    "linter:js": "npx eslint --config .eslintrc.js 'src/**/*.{ts,tsx}' --max-warnings 0",
    "linter:css": "npx stylelint src/**/*.css",
    "linter": "npm run linter:js && npm run linter:css",
    "linter:fix": "npm run linter:js -- --fix --format codeFrame && npm run linter:css --fix",
    "start": "PORT=3003 react-scripts start",
    "pretest": "tsc --noEmit",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "react-scripts test --verbose=false",
    "bundle": "npm run build && source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "auth0-js": "^9.12.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "formik": "^2.1.0",
    "formik-material-ui": "^1.0.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.13.0",
    "typescript": "3.7.4",
    "yup": "^0.28.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.2",
    "@types/auth0-js": "^9.10.7",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/ramda": "^0.26.39",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/recompose": "^0.30.7",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "coveralls": "^3.0.9",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "pact": "^4.3.2",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.0",
    "source-map-explorer": "^2.2.1",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0"
  },
  "proxy": "http://localhost:4003",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --config .eslintrc.js --max-warnings 0",
    "*.scss": "./go linter-css"
  }
}
